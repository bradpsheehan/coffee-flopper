<%= render partial: 'layouts/find_user'%>
<%= render partial: 'layouts/instagram_images'%>

<% @shops.each do |shop| %>
  <div class="media">
    <a class="pull-left" href="#">
      <%= review(shop) %>
    </a>
    <div class="media-body">
      <h4 class="media-heading">
        <%= link_to shop.name, shop %>
        <%= shop.website %>
      </h4>
      <div class="media">
          <strong>.<%= wifi_helper(shop) %>.</strong>
          <br/>
          <strong>.<%= group_helper(shop) %>.</strong>
        <div class="media-body">
          <i><%= shop.yelp.snippet_text %></i>
        </div>
      </div>
      
    </div>
  </div>
<% end %>
